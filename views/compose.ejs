<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <label>Title</label>
    <input class ="form-control" type="text" name="newTitle">
    <label>Post</label>
    <textarea class ="form-control" name="newBody" rows="5" cols="30"></textarea>
    <button class="btn btn-primary" type="submit" name="button" value="<%= composeItems %>">Publish</button>
</form>

<%- include("partials/footer"); -%>